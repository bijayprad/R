# Define the data as a vector
data <- c(
  12, 40, 22, 10, 32,
  15, 42, 23, 14, 34,
  14, 39, 24, 12, 31,
  18, 44, 26, 16, 36,
  20, 47, 29, 19, 38,
  21, 46, 27, 18, 39,
  17, 41, 25, 13, 33,
  19, 45, 28, 17, 37,
  13, 38, 21, 11, 30,
  16, 43, 24, 15, 35
)

# Convert to a 10x5 matrix (10 rows, 5 columns by row)
X <- matrix(data, nrow = 10, byrow = TRUE)

# Set column names
colnames(X) <- c("X1", "X2", "X3", "X4", "X5")

