# Create the matrix data (row-wise)
data_matrix <- matrix(c(
  5, 3, 2, 3, 4,  2, 
  2, 1, 4, 3, 2, 4, 
  4, 3, 2, 4, 1, 4, 
  3, 5, 1, 2, 3, 2, 
  2, 1, 5, 4, 2, 4, 
  3, 2, 4, 3, 4, 1
), nrow = 6, byrow = TRUE)
data_matrix
# Assign column names
colnames(data_matrix) <- c("A1", "A2", "A3", "D1", "D2", "D3")
